<style>
    body{
        font-family: 'Roboto Slab', serif;
    }
    #colorTable td {
        height: 34px;
        width: 100px;
        vertical-align: middle;
        text-align: center;
    }
    #colorTable td:before {
        content: attr(class);
    }
    #colorTable th {
        line-height: 68px;
        width: 20%;
        border: solid 1px #f0f0f0;
    }
    .grid > div {
        background: #ccc;
        text-align: center;
        line-height: 32px;
        overflow: hidden;
        white-space: nowrap;
    }
    .grid > div:before {
        content: attr(class);
    }
    body {
        margin: 0 auto;
    }
    #icons > a {
        display: block;
        float: left;
        width: calc(10% - 10px);
        margin: 5px;
        text-align: center;
        background-color: #f0f0f0;
        padding: 20px 0;
        text-transform: capitalize;
    }
    #icons > a:hover {
        background-color: #ccc;
    }
    #icons > a:before {
        font-size: 3em;
        display: block;
        width: 100%;
        float: left;
    }
    #icons > a[class$="-star"]:not(.icon-star) {
        width: calc(20% - 10px);
    }
    #helperClasses > h3 {
        width: auto;
        width: 50%;
        margin: 5px 0;
    }
    @media (max-width: 768px){
        #icons > a {
            width: calc(33.33333% - 10px);
        }
        #icons > a[class$="-star"]:not(.icon-star) {
            width: calc(100% - 10px);
        }
    }
</style>
